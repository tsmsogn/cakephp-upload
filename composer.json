{
    "name": "tsmsogn/cakephp-upload",
    "description": "CakephpUpload plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "cakephp/cakephp": "^3.4",
        "codeguy/upload": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Tsmsogn\\CakephpUpload\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tsmsogn\\CakephpUpload\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
